<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\htmlspecialchars_decode()
 *
 * @link https://secure.php.net/htmlspecialchars_decode
 *
 * NOTES: same as \htmlspecialchars_decode() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function htmlspecialchars_decode($string, $quote_style = null)
{
    return \htmlspecialchars_decode($string, $quote_style = null);
}