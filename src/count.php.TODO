<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\count()
 *
 * @link https://secure.php.net/count
 *
 * NOTES: same as \count() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function count($var, $mode = COUNT_NORMAL)
{
    return \count($var, $mode = COUNT_NORMAL);
}