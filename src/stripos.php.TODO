<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\stripos()
 *
 * @link https://secure.php.net/stripos
 *
 * NOTES: same as \stripos() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function stripos($haystack, $needle, $offset = null)
{
    return \stripos($haystack, $needle, $offset = null);
}