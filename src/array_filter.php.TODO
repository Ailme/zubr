<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\array_filter()
 *
 * @link https://secure.php.net/array_filter
 *
 * NOTES: same as \array_filter() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function array_filter(array $input, $callback = null, $flag = 0)
{
    return \array_filter();
}