<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\unserialize()
 *
 * @link https://secure.php.net/unserialize
 *
 * NOTES: same as \unserialize() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function unserialize($str, array $options = null)
{
    return \unserialize($str, array $options = null);
}