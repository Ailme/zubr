<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\sha1_file()
 *
 * @link https://secure.php.net/sha1_file
 *
 * NOTES: same as \sha1_file() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function sha1_file($filename, $raw_output = null)
{
    return \sha1_file($filename, $raw_output = null);
}