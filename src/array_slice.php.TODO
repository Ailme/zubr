<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\array_slice()
 *
 * @link https://secure.php.net/array_slice
 *
 * NOTES: same as \array_slice() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function array_slice(array $array, $offset, $length = null, $preserve_keys = false)
{
    return \array_slice(array $array, $offset, $length = null, $preserve_keys = false);
}