<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\bindec()
 *
 * @link https://secure.php.net/bindec
 *
 * NOTES: same as \bindec() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function bindec($binary_string)
{
    return \bindec($binary_string);
}