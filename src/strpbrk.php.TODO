<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\strpbrk()
 *
 * @link https://secure.php.net/strpbrk
 *
 * NOTES: same as \strpbrk() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function strpbrk($haystack, $char_list)
{
    return \strpbrk($haystack, $char_list);
}