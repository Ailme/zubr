<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\asort()
 *
 * @link https://secure.php.net/asort
 *
 * NOTES: same as \asort() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function asort(array &$array, $sort_flags = null)
{
    return \asort(array &$array, $sort_flags = null);
}