<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\arsort()
 *
 * @link https://secure.php.net/arsort
 *
 * NOTES: same as \arsort() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function arsort(array &$array, $sort_flags = null)
{
    return \arsort(array &$array, $sort_flags = null);
}