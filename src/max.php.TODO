<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\max()
 *
 * @link https://secure.php.net/max
 *
 * NOTES: same as \max() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function max(array $value1, $value2 = null, ...$values)
{
    return \max(array $value1, $value2 = null, ...$values);
}