<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\strcoll()
 *
 * @link https://secure.php.net/strcoll
 *
 * NOTES: same as \strcoll() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function strcoll($str1, $str2)
{
    return \strcoll($str1, $str2);
}