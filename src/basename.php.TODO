<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\basename()
 *
 * @link https://secure.php.net/basename
 *
 * NOTES: same as \basename() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function basename($path, $suffix = null)
{
    return \basename($path, $suffix = null);
}