<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\set_time_limit()
 *
 * @link https://secure.php.net/set_time_limit
 *
 * NOTES: same as \set_time_limit() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function set_time_limit($seconds)
{
    return \set_time_limit($seconds);
}