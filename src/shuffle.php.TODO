<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\shuffle()
 *
 * @link https://secure.php.net/shuffle
 *
 * NOTES: same as \shuffle() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function shuffle(array &$array)
{
    return \shuffle(array &$array);
}