<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\call_user_func()
 *
 * @link https://secure.php.net/call_user_func
 *
 * NOTES: same as \call_user_func() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function call_user_func($function, $parameter = null, $_ = null)
{
    return \call_user_func($function, $parameter = null, $_ = null);
}