<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\explode()
 *
 * @link https://secure.php.net/explode
 *
 * NOTES: same as \explode() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function explode($delimiter, $string, $limit = null)
{
    return \explode($delimiter, $string, $limit = null);
}