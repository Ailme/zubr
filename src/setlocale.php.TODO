<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\setlocale()
 *
 * @link https://secure.php.net/setlocale
 *
 * NOTES: same as \setlocale() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function setlocale($category, $locale, $_ = null)
{
    return \setlocale($category, $locale, $_ = null);
}