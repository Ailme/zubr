<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\quoted_printable_encode()
 *
 * @link https://secure.php.net/quoted_printable_encode
 *
 * NOTES: same as \quoted_printable_encode() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function quoted_printable_encode($str)
{
    return \quoted_printable_encode($str);
}