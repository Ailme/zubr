<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\levenshtein()
 *
 * @link https://secure.php.net/levenshtein
 *
 * NOTES: same as \levenshtein() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function levenshtein($str1, $str2, $cost_ins = null, $cost_rep = null, $cost_del = null)
{
    return \levenshtein($str1, $str2, $cost_ins = null, $cost_rep = null, $cost_del = null);
}