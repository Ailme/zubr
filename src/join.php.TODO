<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\join()
 *
 * @link https://secure.php.net/join
 *
 * NOTES: same as \join() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function join($glue = "", $pieces)
{
    return \join($glue = "", $pieces);
}