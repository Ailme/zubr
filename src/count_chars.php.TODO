<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\count_chars()
 *
 * @link https://secure.php.net/count_chars
 *
 * NOTES: same as \count_chars() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function count_chars($string, $mode = null)
{
    return \count_chars($string, $mode = null);
}