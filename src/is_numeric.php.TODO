<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\is_numeric()
 *
 * @link https://secure.php.net/is_numeric
 *
 * NOTES: same as \is_numeric() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function is_numeric($var)
{
    return \is_numeric($var);
}