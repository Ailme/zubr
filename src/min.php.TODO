<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\min()
 *
 * @link https://secure.php.net/min
 *
 * NOTES: same as \min() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function min(array $value1, $value2 = null, ...$values)
{
    return \min(array $value1, $value2 = null, ...$values);
}