<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\strrpos()
 *
 * @link https://secure.php.net/strrpos
 *
 * NOTES: same as \strrpos() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function strrpos($haystack, $needle, $offset = 0)
{
    return \strrpos($haystack, $needle, $offset = 0);
}