<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\ksort()
 *
 * @link https://secure.php.net/ksort
 *
 * NOTES: same as \ksort() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function ksort(array &$array, $sort_flags = null)
{
    return \ksort(array &$array, $sort_flags = null);
}