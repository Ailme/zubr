<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\vprintf()
 *
 * @link https://secure.php.net/vprintf
 *
 * NOTES: same as \vprintf() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function vprintf($format, array $args)
{
    return \vprintf($format, array $args);
}