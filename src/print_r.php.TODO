<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\print_r()
 *
 * @link https://secure.php.net/print_r
 *
 * NOTES: same as \print_r() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function print_r($expression, $return = null)
{
    return \print_r($expression, $return = null);
}