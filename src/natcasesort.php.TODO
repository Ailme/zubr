<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\natcasesort()
 *
 * @link https://secure.php.net/natcasesort
 *
 * NOTES: same as \natcasesort() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function natcasesort(array &$array)
{
    return \natcasesort(array &$array);
}