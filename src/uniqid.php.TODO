<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\uniqid()
 *
 * @link https://secure.php.net/uniqid
 *
 * NOTES: same as \uniqid() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function uniqid($prefix = "", $more_entropy = false)
{
    return \uniqid($prefix = "", $more_entropy = false);
}