<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\microtime()
 *
 * @link https://secure.php.net/microtime
 *
 * NOTES: same as \microtime() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function microtime($get_as_float = null)
{
    return \microtime($get_as_float = null);
}