<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\intval()
 *
 * @link https://secure.php.net/intval
 *
 * NOTES: same as \intval() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function intval($var, $base = null)
{
    return \intval($var, $base = null);
}