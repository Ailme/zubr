<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\rtrim()
 *
 * @link https://secure.php.net/rtrim
 *
 * NOTES: same as \rtrim() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function rtrim($str, $charlist = " \t\n\r\0\x0B")
{
    return \rtrim($str, $charlist = " \t\n\r\0\x0B");
}