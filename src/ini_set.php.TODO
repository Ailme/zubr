<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\ini_set()
 *
 * @link https://secure.php.net/ini_set
 *
 * NOTES: same as \ini_set() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function ini_set($varname, $newvalue)
{
    return \ini_set($varname, $newvalue);
}