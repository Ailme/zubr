<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\gettimeofday()
 *
 * @link https://secure.php.net/gettimeofday
 *
 * NOTES: same as \gettimeofday() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function gettimeofday($return_float = null)
{
    return \gettimeofday($return_float = null);
}