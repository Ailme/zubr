<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\sprintf()
 *
 * @link https://secure.php.net/sprintf
 *
 * NOTES: same as \sprintf() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function sprintf($format, $args = null, $_ = null)
{
    return \sprintf($format, $args = null, $_ = null);
}