<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\is_infinite()
 *
 * @link https://secure.php.net/is_infinite
 *
 * NOTES: same as \is_infinite() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function is_infinite($val)
{
    return \is_infinite($val);
}