<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\printf()
 *
 * @link https://secure.php.net/printf
 *
 * NOTES: same as \printf() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function printf($format, $args = null, $_ = null)
{
    return \printf($format, $args = null, $_ = null);
}