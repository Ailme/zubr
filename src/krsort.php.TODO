<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\krsort()
 *
 * @link https://secure.php.net/krsort
 *
 * NOTES: same as \krsort() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function krsort(array &$array, $sort_flags = null)
{
    return \krsort(array &$array, $sort_flags = null);
}