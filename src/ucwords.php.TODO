<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\ucwords()
 *
 * @link https://secure.php.net/ucwords
 *
 * NOTES: same as \ucwords() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function ucwords($str, $delimiters = " \t\r\n\f\v")
{
    return \ucwords($str, $delimiters = " \t\r\n\f\v");
}