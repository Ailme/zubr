<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\base64_decode()
 *
 * @link https://secure.php.net/base64_decode
 *
 * NOTES: same as \base64_decode() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function base64_decode($data, $strict = null)
{
    return \base64_decode($data, $strict = null);
}