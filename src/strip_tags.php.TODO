<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\strip_tags()
 *
 * @link https://secure.php.net/strip_tags
 *
 * NOTES: same as \strip_tags() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function strip_tags($str, $allowable_tags = null)
{
    return \strip_tags($str, $allowable_tags = null);
}