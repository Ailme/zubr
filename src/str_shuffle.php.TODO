<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\str_shuffle()
 *
 * @link https://secure.php.net/str_shuffle
 *
 * NOTES: same as \str_shuffle() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function str_shuffle($str)
{
    return \str_shuffle($str);
}