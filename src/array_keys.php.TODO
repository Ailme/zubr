<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\array_keys()
 *
 * @link https://secure.php.net/array_keys
 *
 * NOTES: same as \array_keys() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function array_keys(array $input, $search_value = null, $strict = null)
{
    return \array_keys();
}