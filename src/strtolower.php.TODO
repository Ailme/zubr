<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\strtolower()
 *
 * @link https://secure.php.net/strtolower
 *
 * NOTES: same as \strtolower() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function strtolower($str)
{
    return \strtolower($str);
}