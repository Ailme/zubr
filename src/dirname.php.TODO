<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\dirname()
 *
 * @link https://secure.php.net/dirname
 *
 * NOTES: same as \dirname() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function dirname($path, $levels = 1)
{
    return \dirname($path, $levels = 1);
}