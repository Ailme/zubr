<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\strnatcasecmp()
 *
 * @link https://secure.php.net/strnatcasecmp
 *
 * NOTES: same as \strnatcasecmp() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function strnatcasecmp($str1, $str2)
{
    return \strnatcasecmp($str1, $str2);
}