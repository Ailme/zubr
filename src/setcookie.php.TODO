<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\setcookie()
 *
 * @link https://secure.php.net/setcookie
 *
 * NOTES: same as \setcookie() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function setcookie($name, $value = "", $expire = 0, $path = "", $domain = "", $secure = false, $httponly = false)
{
    return \setcookie($name, $value = "", $expire = 0, $path = "", $domain = "", $secure = false, $httponly = false);
}