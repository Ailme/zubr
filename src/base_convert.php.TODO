<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\base_convert()
 *
 * @link https://secure.php.net/base_convert
 *
 * NOTES: same as \base_convert() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function base_convert($number, $frombase, $tobase)
{
    return \base_convert($number, $frombase, $tobase);
}