<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\convert_cyr_string()
 *
 * @link https://secure.php.net/convert_cyr_string
 *
 * NOTES: same as \convert_cyr_string() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function convert_cyr_string($str, $from, $to)
{
    return \convert_cyr_string($str, $from, $to);
}