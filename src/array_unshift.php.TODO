<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\array_unshift()
 *
 * @link https://secure.php.net/array_unshift
 *
 * NOTES: same as \array_unshift() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function array_unshift(array &$array, $var, $_ = null)
{
    return \array_unshift(array &$array, $var, $_ = null);
}