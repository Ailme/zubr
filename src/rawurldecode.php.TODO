<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\rawurldecode()
 *
 * @link https://secure.php.net/rawurldecode
 *
 * NOTES: same as \rawurldecode() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function rawurldecode($str)
{
    return \rawurldecode($str);
}