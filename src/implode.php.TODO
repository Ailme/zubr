<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\implode()
 *
 * @link https://secure.php.net/implode
 *
 * NOTES: same as \implode() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function implode($glue = "", array $pieces)
{
    return \implode($glue = "", array $pieces);
}