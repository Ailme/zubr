<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\array_shift()
 *
 * @link https://secure.php.net/array_shift
 *
 * NOTES: same as \array_shift() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function array_shift(array &$array)
{
    return \array_shift(array &$array);
}