<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\vsprintf()
 *
 * @link https://secure.php.net/vsprintf
 *
 * NOTES: same as \vsprintf() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function vsprintf($format, array $args)
{
    return \vsprintf($format, array $args);
}