<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\array_rand()
 *
 * @link https://secure.php.net/array_rand
 *
 * NOTES: same as \array_rand() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function array_rand(array $input, $num_req = null)
{
    return \array_rand();
}