<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\var_export()
 *
 * @link https://secure.php.net/var_export
 *
 * NOTES: same as \var_export() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function var_export($expression, $return = null)
{
    return \var_export($expression, $return = null);
}