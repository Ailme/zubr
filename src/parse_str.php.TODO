<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\parse_str()
 *
 * @link https://secure.php.net/parse_str
 *
 * NOTES: same as \parse_str() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function parse_str($str, array &$arr = null)
{
    return \parse_str($str, array &$arr = null);
}