<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\money_format()
 *
 * @link https://secure.php.net/money_format
 *
 * NOTES: same as \money_format() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function money_format($format, $number)
{
    return \money_format($format, $number);
}