<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\is_integer()
 *
 * @link https://secure.php.net/is_integer
 *
 * NOTES: same as \is_integer() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function is_integer($var)
{
    return \is_integer($var);
}