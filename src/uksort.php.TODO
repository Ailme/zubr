<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\uksort()
 *
 * @link https://secure.php.net/uksort
 *
 * NOTES: same as \uksort() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function uksort(array &$array, $cmp_function)
{
    return \uksort(array &$array, $cmp_function);
}