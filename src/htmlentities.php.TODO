<?php
declare(strict_types=1);

namespace Zubr;

/**
 * \Zubr\htmlentities()
 *
 * @link https://secure.php.net/htmlentities
 *
 * NOTES: same as \htmlentities() || in Zubr, $param_1 goes before $param_2, and we return an array instead of void
*/
function htmlentities($string, $quote_style = null, $charset = null, $double_encode = true)
{
    return \htmlentities($string, $quote_style = null, $charset = null, $double_encode = true);
}